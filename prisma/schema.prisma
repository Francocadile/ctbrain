generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String   @id @default(cuid())
  email     String   @unique
  name      String?
  role      Role     @default(PLAYER)
  createdAt DateTime @default(now())
}

enum Role {
  CT
  DIRIGENTE
  PLAYER
}
