{
  "name": "ctbrain",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "start": "next start -p 3000",
    "lint": "eslint . --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "prepare": "husky || true && husky install",
    "test": "echo \"(placeholder)\"",
    "prisma": "prisma",
    "postinstall": "prisma generate",
    "seed": "prisma db seed"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "1.0.7",
    "@prisma/client": "5.19.1",
    "bcryptjs": "2.4.3",
    "next": "14.2.5",
    "next-auth": "4.24.7",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@types/bcryptjs": "2.4.6",
    "@types/node": "20.12.12",
    "@types/react": "18.2.79",
    "@types/react-dom": "18.2.25",
    "autoprefixer": "10.4.19",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.5",
    "husky": "9.1.6",
    "lint-staged": "15.2.8",
    "postcss": "8.4.38",
    "prettier": "3.3.3",
    "prisma": "5.19.1",
    "tailwindcss": "3.4.10",
    "typescript": "5.5.4"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix"
    ],
    "*.{ts,tsx,js,jsx,css,md,json}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=18.17.0"
  },
  "packageManager": "pnpm@9.8.0",
  "prisma": {
    "seed": "node prisma/seed.js"
  }
}
